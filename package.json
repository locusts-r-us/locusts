{
  "name": "locusts-monorepo",
  "version": "0.0.0",
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "prettier": "^3.2.5",
    "syncpack": "13.0.0",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.11.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ci:format": "prettier --check \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "publish-packages": "turbo build && bash ./contrib/tag-repo && changeset publish",
    "versioning": "changeset version && bash ./contrib/version-bump"
  }
}
